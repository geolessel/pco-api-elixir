      "attributes": {
        "body": "<html>\r\n  <head>\r\n    <title>Phone Numbers<\/title>\r\n    {{ helpers.bootstrap_3 }}\r\n    <style>\r\n      @page { margin: 0.5in; size: US-Letter; }\r\n      body { font-family: \"Lucida Grande\", \"Lucida Sans Unicode\", Helvetica, Arial, Verdana, sans-serif; }\r\n    <\/style>\r\n  <\/head>\r\n  <body>\r\n    <h1>\r\n      {{ filter.name }}\r\n    <\/h1>\r\n    <table class=\"table table-condensed phone_numbers\">\r\n      <thead>\r\n        <tr>\r\n          <th>Name<\/th>\r\n          <th>Phone Number<\/th>\r\n        <\/tr>\r\n      <\/thead>\r\n\r\n      <tbody>\r\n        {% for person in people %}\r\n          <tr>\r\n            <td>{{ person.name }}<\/td>\r\n            <td>\r\n              {% for phone_number in person.phone_numbers %}\r\n                {{ phone_number.location }}: {{ phone_number.number }}\r\n              {% endfor %}\r\n            <\/td>\r\n          <\/tr>\r\n        {% endfor %}\r\n      <\/tbody>\r\n    <\/table>\r\n  <\/body>\r\n<\/html>",
